Summary: Managing daemon for applications running within a VM (part of XenBEE)
Name: xbeinstd
Version: @VERSION@
Release: 1
Source0: %{name}-%{version}.tar.gz
License: GPL
Group: Libraries
Vendor: Fraunhofer ITWM
URL: http://www.xenbee.net/
Packager: Alexander Petry <petry@itwm.fhg.de>
BuildRoot: %{_builddir}/%{name}-root
#Conflicts: xbeinstd < 0.0.0, xmlbeansxx-gen
%description
TODO

%package devel
Summary: TODO
Group: Libraries
Requires: xbeinstd = %{version}-%{release}
#Conflicts: 
%description devel
TODO

%prep
%setup -q

%build
# bootstrap is here because of gentormake
sh ./bootstrap
sh ./configure --prefix=/usr # --enable-ltdl-convenience
make $JOBS

%install
rm -rf $RPM_BUILD_ROOT
make DESTDIR=$RPM_BUILD_ROOT install

%clean
rm -rf $RPM_BUILD_ROOT

%files
%defattr(-, root, root)
%{_libdir}/*.so.*

%files devel
%defattr(-, root, root)
%{_libdir}/mqs-@VERSION@/*
%{_libdir}/*.so
%{_libdir}/*.a
%{_libdir}/*.la
%{_includedir}/mqs/
%{_includedir}/xbe/
%{_datadir}/xbeinstd/

